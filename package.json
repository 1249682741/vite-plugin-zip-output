{
  "name": "vite-plugin-zip-output",
  "version": "0.0.3",
  "description": "a vite plugin to compress output folder to zip",
  "author": "pikachu",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc src/index.ts --watch",
    "build": "rollup -c"
  },
  "type": "module",
  "keywords": [
    "zip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/1249682741/vite-plugin-zip-output.git"
  },
  "engines": {
    "node": "^14.18.0 || >= 16.0.0"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^3.0.0"
  },
  "peerDependencies": {
    "vite": "^3.0.0"
  }
}
