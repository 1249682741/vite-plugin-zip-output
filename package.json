{
  "name": "vite-plugin-zip-output",
  "version": "0.0.15",
  "description": "a vite plugin to compress output folder to zip",
  "author": "pikachu",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs"
  },
  "type": "module",
  "keywords": [
    "zip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/1249682741/vite-plugin-zip-output.git"
  },
  "engines": {
    "node": "^14.18.0 || >= 16.0.0"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "nodemailer": "^6.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/archiver": "^5.3.2",
    "@types/node": "^20.4.1",
    "@types/nodemailer": "^6.4.8",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^3.0.0"
  },
  "peerDependencies": {
    "vite": "^3.0.0"
  }
}
